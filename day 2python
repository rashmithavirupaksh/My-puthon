if a>=18:
  print("eligibility for voting")
else:
  print("not eligibility for voting")# grade calculation
mark=int(input("enter the marks"))
if mark>=90:
  print("grade a")
elif mark>=75:
  print("grade b")
elif mark>=50:
  print("grade c")
else:
  print("fail")
alpha=str(input("enter the alphabet"))
if alpha in "aeiou":
  print("the alphabet is a vowel")
else:
  print("the alphabet is a consonant")num=int(input("enter a number:"))
if num%3==0:
  print(num,"is divisible by both 3 and 5")
elif num%5==0:
  print(num,"is divisible by both 3 and 5")
else:
  print(num,"is not divisible by both 3 and 5")
