x=int(input("enter the number"))
if x%4==0:
  print("it is even number")
else:
  print("it is odd number")#if elif else
mark=int(input("enter the marks"))
if mark>=90:
  print("excellent")
elif mark>=80:
  print("good")
elif mark>=70:
  print("average")
else:
  print("fail")# if condition
a=int(input("enter the age"))
if a>=18:
  print("eligible to vote")# if elif condition
a=int(input("enter the number"))
if a>90:
  print("the number is positive")
elif a<60:
  print("the number is negative")
else:
  print("the number is zero")# if number is positive or negative or zero
a=float(input("enter the number"))
if a>0:
  print("positive number")
if a<0:
  print("negative number")
else:
  print("it is zero")# largest two numbers
num1=int(input("enter first number"))
num2=int(input("enter second number"))
if num1>num2:
  print(num1,"it is largest number")
elif num1<num2:
  print(num2,"it is largest number")
else:
  print("both number equal")# largest three numbers
num1=int(input("enter the first number"))
num2=int(input("enter the second number"))
num3=int(input("enter the third number"))
if num1<num2:
  print(num2,"the largest number is")
elif num2<num3:
  print(num3,"the largest number is")
else:
  print(num1,"the largest number is")for a in range(1,10):
  if a%2==0:
    print("even number")
  else:
    print("odd number")# leap year
year=int(input("enter the year"))
if year%4==0:
  print(year,"is a leap year")
else:
  print(year,"is a leap year")# voting eligibility
